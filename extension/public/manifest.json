{
	"name": "8kur",
	"version": "0.1",
	"manifest_version": 3,
	"action": {
		"default_title": "8kur import",
		"default_popup": "index.html"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"contextMenus",
		"scripting",
		"storage",
		"webNavigation"
	],
	"host_permissions": ["*://news.ycombinator.com/favorites/*"],
	"background": {
		"service_worker": "sw.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": ["*://news.ycombinator.com/*"],
			"js": ["hn-content-script.js"],
			"run_at": "document_end"
		  }
	]
}
